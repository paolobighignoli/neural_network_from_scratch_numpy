    "takedowns = np.array([2.0, 3.0, 1.0])\n",
    "sign_strikes = np.array([0.17, 0.8, 0.9])\n",
    "sub_att = np.array([1.0, 0.0, 3.0])\n",
    "\n",
    "weights = np.array([\n",
    "    [2.33, 8.0, 1.0],\n",
    "    [0.33, 0.9, 1.0],\n",
    "    [0.55, 0.17, 0.88]\n",
    "])\n",
    "\n",
    "goal_pred = np.array([0.9, 0.8, 0.3])\n",
    "\n",
    "alpha = 0.01\n",
    "\n",
    "input = np.array([takedowns[0], sign_strikes[0], sub_att[0]])\n",
    "\n",
    "def neural_network(input, weights):\n",
    "\n",
    "    return weights.dot(input)\n",
    "\n",
    "pred = neural_network(input, weights)\n",
    "\n",
    "it = 1\n",
    "max_it = 100\n",
    "\n",
    "err = 100\n",
    "\n",
    "alpha = 0.01\n",
    "\n",
    "while it < max_it and err > 0.01:\n",
    "\n",
    "    pred = neural_network(input, weights) # 3x1\n",
    "\n",
    "    delta = (pred - goal_pred) # 3x1\n",
    "\n",
    "    err = np.mean(delta**2) # 1x1\n",
    "\n",
    "    weights_delta = delta.reshape(3, 1).dot(input.reshape(1, 3)) # 3x3\n",
    "\n",
    "    weights = weights - np.multiply(alpha, weights_delta)\n",
    "\n",
    "    print(\"Iteration: \", it)\n",
    "    print(\"Prediction: \", pred)\n",
    "    print(\"Goal prediction: \", goal_pred)\n",
    "    print(\"Weights: \", weights)\n",
    "    print(\"Error: \", err)\n",
    "    print(\"-\"*10)\n",
    "\n",
    "    it += 1\n",
    "    \n",
    "\n"
   ]
