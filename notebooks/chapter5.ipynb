   "source": [
    "takedowns = np.array([1.0, 3.0, 1.0])\n",
    "sign_strikes = np.array([0.0, 0.8, 0.9])\n",
    "sub_att = np.array([1.0, 0.0, 3.0])\n",
    "\n",
    "weights = np.array([7.0, 0.5, 0.0])\n",
    "\n",
    "goal_pred = 1.0\n",
    "\n",
    "alpha = 0.1\n",
    "\n",
    "input = np.array([takedowns[0], sign_strikes[0], sub_att[0]])\n",
    "\n",
    "def neural_network(input, weights):\n",
    "\n",
    "    return input.dot(weights)\n",
    "\n",
    "it = 1\n",
    "max_it = 100\n",
    "\n",
    "err = 100\n",
    "\n",
    "while it < max_it and err > 0.001:\n",
    "    \n",
    "    pred = neural_network(input, weights)\n",
    "\n",
    "    err = (pred - goal_pred)**2\n",
    "\n",
    "    weights_delta = np.multiply((pred - goal_pred), input)\n",
    "    \n",
    "    # # freeze w_2\n",
    "    # weights_delta[2] = 0\n",
    "\n",
    "    print(\"Iteration: \", it)\n",
    "    print(\"Prediction: \", pred)\n",
    "    print(\"Goal prediction: \", goal_pred)\n",
    "    print(\"Error: \", err)\n",
    "    print(\"Input: \", input)\n",
    "    print(\"Weights: \", weights)\n",
    "    print(\"Weights delta: \", weights_delta)\n",
    "    print(\"-\"*10)\n",
    "\n",
    "    weights = weights - np.multiply(alpha, weights_delta)\n",
    "\n",
    "    it += 1\n"
   ]
